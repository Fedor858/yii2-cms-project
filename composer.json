{
    "name": "bariew/yii2-cms-project",
    "description": "Yii2 Null CMS",
    "keywords": ["yii2", "cms", "basic"],
    "homepage": "http://www.yii2.anabena.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "forum": "http://www.yiiframework.com/forum/",
        "source": "https://github.com/bariew/yii2-cms-project"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "bower-asset/bootstrap": "3.2.*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "bariew/yii2-module-cms-module": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::setPermission",
            "bariew\\moduleModule\\ModuleBootstrap::setPermission"
        ]
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "writable": [
            "config/local",
            "runtime",
            "web/assets",
            "web/files",
            "web/themes"
        ],
        "executable": [
            "yii"
        ],
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
